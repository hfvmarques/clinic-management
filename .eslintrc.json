{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "function-paren-newline": "off",
    "prefer-const": "off",
    "implicit-arrow-linebreak": "off",
    "comma-dangle": [
      "error",
      {
        "objects": "ignore",
        "arrays": "ignore"
      }
    ]
  }
}
